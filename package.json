{
  "name": "curso-angular-multiplataforma-open-webinars",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/apps/albums-electron/main.js",
  "author": {
    "email": "jonatanlucasmolina@gmail.com",
    "name": "Jonatan Lucas"
  },
  "description": "Monorepo para el curso de Angular en la Multiplataforma de OpenWebinars",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "start:web": "nx serve albums-web --port 4210",
    "start:desktop": "npm run build:desktop && electron .",
    "build": "nx build",
    "build:web": "nx build albums-web",
    "build-prod:web": "nx build albums-web --prod",
    "build:desktop": "ng build --prod --base-href ./ && ng build albums-electron",
    "electron:linux": "npm run build:desktop && electron-builder build --linux",
    "test": "nx test",
    "format:write": "prettier --ignore-path .gitignore --write .",
    "lint:web": "ng lint albums-web",
    "server:pwa": "npm run build-prod:web && angular-http-server --open -p 8880 --path ./dist/apps/albums-web"
  },
  "private": true,
  "build": {
    "appId": "electron.app.angularmultiplataforma",
    "files": [
      "dist/apps/albums-web/**/*",
      "node_modules/**/*",
      "package.json",
      "dist/apps/albums-electron/main.js"
    ],
    "directories": {
      "output": "dist/desktop"
    }
  },
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.5",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.5",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/pwa": "^13.2.5",
    "@angular/router": "~13.2.0",
    "@angular/service-worker": "~13.2.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/language-service": "~13.2.0",
    "@nrwl/angular": "^13.8.3",
    "@nrwl/cli": "13.8.3",
    "@nrwl/cypress": "13.8.3",
    "@nrwl/eslint-plugin-nx": "13.8.3",
    "@nrwl/jest": "13.8.3",
    "@nrwl/linter": "13.8.3",
    "@nrwl/node": "^13.8.5",
    "@nrwl/tao": "13.8.3",
    "@nrwl/workspace": "13.8.3",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "angular-http-server": "^1.10.0",
    "cypress": "^9.1.0",
    "electron": "^17.1.1",
    "electron-builder": "^22.14.13",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2"
  }
}
